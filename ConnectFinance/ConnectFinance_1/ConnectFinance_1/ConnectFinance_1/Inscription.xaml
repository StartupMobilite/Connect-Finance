<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ConnectFinance_1.Inscription">
	<ContentPage.Content>
		<StackLayout BackgroundColor="White">
		<StackLayout.Children>
			<Label Text="Logo" VerticalOptions="Start"/>
			<StackLayout VerticalOptions="CenterAndExpand">
				<StackLayout.Children>
					<Button x:Name="btnEntrepreneur" Text="Je suis un nouvel entrepreneur" Clicked="BtnEntrepreneur_OnClicked"></Button>
					<Button x:Name="btnInvestisseur" Text="Je suis un nouvel investisseur" Clicked="BtnInvestisseur_OnClicked"></Button>
				</StackLayout.Children>
			</StackLayout>
		</StackLayout.Children>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>